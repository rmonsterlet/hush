    "http-server": "^0.11.1",
    "moment": "^2.22.1",
    "ng-pwa-tools": "0.0.15",
    "orbit-controls-es6": "^1.0.12",
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0",
    "sha256": "^0.2.0",
    "three": "^0.137.0",
    "three-fbx-loader": "^1.0.3",
    "three-mtl-loader": "^1.0.2",
