{
  "name": "hush-front",
  "version": "1.0.0",
  "description": "Hush",
  "license": "MIT",
  "author": "Romain MONSTERLET",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --verbose true --stats-json",
    "build.prod": "ng build --prod",
    "start.prod": "http-server dist -p 4200",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "copy node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "build-prod-ngsw": "ng build --prod && npm run ngsw-config && npm run ngsw-copy",
    "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist -p 4200",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "i18n": "ng-xi18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@angular/service-worker": "^5.2.9",
    "@swimlane/ngx-datatable": "^11.3.2",
    "file-saver": "^1.3.8",
    "firebase": "^4.12.1",
    "firebaseui": "^2.7.0",
    "hammerjs": "^2.0.8",
    "http-server": "^0.11.1",
    "moment": "^2.22.1",
    "ng-pwa-tools": "0.0.15",
    "rxjs": "^5.5.10",
    "sha256": "^0.2.0",
    "ts-helpers": "^1.1.2",
    "ws": "^5.1.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@types/firebase": "^3.2.1",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "2.8.6",
    "@types/node": "^9.6.5",
    "@types/ws": "4.0.2",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.5",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "npm-check-updates": "^2.14.1",
    "protractor": "~5.3.1",
    "rimraf": "^2.6.2",
    "ts-node": "5.0.1",
    "tslint": "^5.9.1",
    "typescript": "~2.8.1"
  }
